    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^4.9.1"
  },
  "engines": {
    "node": "20.5.1",
    "npm": "9.8.0"
  }
}
